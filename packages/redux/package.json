{
  "name": "@unistash/redux",
  "version": "1.0.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "Redux Toolkit adapter for Unistash - Write once. Stash anywhere.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
