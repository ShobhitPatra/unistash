  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ShobhitPatra/unistash",
    "directory": "packages/redux"
  },
  "homepage": "https://unistash.dev",
  "peerDependencies": {
    "@reduxjs/toolkit": "^2.0.0",
    "react": "^18.0.0",
    "react-redux": "^9.0.0"
  },
  "dependencies": {
    "@unistash/core": "workspace:*"
  },
  "devDependencies": {
    "@reduxjs/toolkit": "^2.0.0",
    "@types/react": "^18.0.0",
    "react": "^18.0.0",
    "react-redux": "^9.0.0",
    "typescript": "^5.0.0"
