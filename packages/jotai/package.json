  "keywords": [
    "unistash",
    "jotai",
    "state-management",
    "react",
    "adapter",
    "atoms"
  ],
  "author": "ShobhitPatra<shobhitpatra.dev@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ShobhitPatra/unistash",
    "directory": "packages/jotai"
  },
  "homepage": "https://unistash.dev",
  "peerDependencies": {
    "jotai": "^2.0.0",
    "react": "^18.0.0"
  },
  "dependencies": {
    "@unistash/core": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "jotai": "^2.0.0",
    "react": "^18.0.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist"
  ]
