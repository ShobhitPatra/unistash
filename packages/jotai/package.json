{
  "name": "@unistash/jotai",
  "version": "0.1.0",
  "description": "Jotai adapter for Unistash - Write once. Stash anywhere.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
